<svg width="600" height="400" viewBox="0 0 600 400" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="600" height="400" fill="url(#heroGradient)"/>
  
  <!-- Gradient Definitions -->
  <defs>
    <linearGradient id="heroGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1e3d94;stop-opacity:0.1"/>
      <stop offset="100%" style="stop-color:#ffd700;stop-opacity:0.1"/>
    </linearGradient>
    <linearGradient id="cardGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#ffffff;stop-opacity:0.9"/>
      <stop offset="100%" style="stop-color:#f3f4f6;stop-opacity:0.9"/>
    </linearGradient>
  </defs>
  
  <!-- Central AI Brain -->
  <circle cx="300" cy="200" r="80" fill="none" stroke="#1e3d94" stroke-width="3" opacity="0.3"/>
  <circle cx="300" cy="200" r="60" fill="none" stroke="#1e3d94" stroke-width="2" opacity="0.5"/>
  <circle cx="300" cy="200" r="40" fill="none" stroke="#ffd700" stroke-width="3" opacity="0.7"/>
  <circle cx="300" cy="200" r="20" fill="#1e3d94" opacity="0.8"/>
  
  <!-- Neural Network Connections -->
  <g stroke="#1e3d94" stroke-width="2" opacity="0.4">
    <line x1="300" y1="120" x2="250" y2="80"/>
    <line x1="300" y1="120" x2="350" y2="80"/>
    <line x1="300" y1="280" x2="250" y2="320"/>
    <line x1="300" y1="280" x2="350" y2="320"/>
    <line x1="220" y1="200" x2="170" y2="180"/>
    <line x1="220" y1="200" x2="170" y2="220"/>
    <line x1="380" y1="200" x2="430" y2="180"/>
    <line x1="380" y1="200" x2="430" y2="220"/>
  </g>
  
  <!-- Node Points -->
  <circle cx="250" cy="80" r="8" fill="#ffd700"/>
  <circle cx="350" cy="80" r="8" fill="#ffd700"/>
  <circle cx="250" cy="320" r="8" fill="#ffd700"/>
  <circle cx="350" cy="320" r="8" fill="#ffd700"/>
  <circle cx="170" cy="180" r="8" fill="#ffd700"/>
  <circle cx="170" cy="220" r="8" fill="#ffd700"/>
  <circle cx="430" cy="180" r="8" fill="#ffd700"/>
  <circle cx="430" cy="220" r="8" fill="#ffd700"/>
  
  <!-- Floating Tech Elements -->
  <g transform="translate(100, 100)">
    <rect width="80" height="50" rx="8" fill="url(#cardGradient)" stroke="#1e3d94" stroke-width="1"/>
    <text x="40" y="25" text-anchor="middle" font-family="Arial" font-size="12" fill="#1e3d94">AI Tools</text>
    <text x="40" y="38" text-anchor="middle" font-family="Arial" font-size="10" fill="#6b7280">Analytics</text>
  </g>
  
  <g transform="translate(420, 100)">
    <rect width="80" height="50" rx="8" fill="url(#cardGradient)" stroke="#ffd700" stroke-width="1"/>
    <text x="40" y="25" text-anchor="middle" font-family="Arial" font-size="12" fill="#1e3d94">Revenue</text>
    <text x="40" y="38" text-anchor="middle" font-family="Arial" font-size="10" fill="#6b7280">Growth</text>
  </g>
  
  <g transform="translate(100, 250)">
    <rect width="80" height="50" rx="8" fill="url(#cardGradient)" stroke="#1e3d94" stroke-width="1"/>
    <text x="40" y="25" text-anchor="middle" font-family="Arial" font-size="12" fill="#1e3d94">Learning</text>
    <text x="40" y="38" text-anchor="middle" font-family="Arial" font-size="10" fill="#6b7280">Platform</text>
  </g>
  
  <g transform="translate(420, 250)">
    <rect width="80" height="50" rx="8" fill="url(#cardGradient)" stroke="#ffd700" stroke-width="1"/>
    <text x="40" y="25" text-anchor="middle" font-family="Arial" font-size="12" fill="#1e3d94">Success</text>
    <text x="40" y="38" text-anchor="middle" font-family="Arial" font-size="10" fill="#6b7280">Stories</text>
  </g>
  
  <!-- Animated Particles -->
  <circle cx="150" cy="150" r="3" fill="#ffd700" opacity="0.6">
    <animate attributeName="opacity" values="0.6;1;0.6" dur="2s" repeatCount="indefinite"/>
  </circle>
  <circle cx="450" cy="150" r="3" fill="#1e3d94" opacity="0.6">
    <animate attributeName="opacity" values="0.6;1;0.6" dur="2.5s" repeatCount="indefinite"/>
  </circle>
  <circle cx="150" cy="250" r="3" fill="#ffd700" opacity="0.6">
    <animate attributeName="opacity" values="0.6;1;0.6" dur="3s" repeatCount="indefinite"/>
  </circle>
  <circle cx="450" cy="250" r="3" fill="#1e3d94" opacity="0.6">
    <animate attributeName="opacity" values="0.6;1;0.6" dur="1.8s" repeatCount="indefinite"/>
  </circle>
</svg>